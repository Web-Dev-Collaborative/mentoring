{
  "extends": [
    "config:base"
  ],
  "dependencyDashboard": true,
  "rebaseConflictedPrs": false,
  "prHourlyLimit": 0,
  "prConcurrentLimit": 3,
  "branchConcurrentLimit": 0,
  "rangeStrategy": "update-lockfile",
  "packageRules": [
    {
      "updateTypes": [
        "minor",
        "patch"
      ],
      "matchCurrentVersion": "!/^0/",
      "automerge": true
    }
  ]
}
